    "@nubeio/tailwind-config": "workspace:*",
    "@nubeio/tsconfig": "workspace:*",
    "@rsbuild/core": "^0.7.9",
    "@rsbuild/plugin-react": "^0.7.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "tailwindcss": "^3.4.4",
